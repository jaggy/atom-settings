"*":
  "atom-autocomplete-php":
    binComposer: "composer"
    binPhp: "/usr/local/php5/bin/php"
  "atom-material-ui":
    colors:
      genAccent: true
    fonts: {}
    tabs:
      compactTabs: true
      tintedTabBar: true
    treeView:
      blendTabs: true
      compactList: true
    ui: {}
  "autocomplete-php": {}
  core:
    disabledPackages: [
      "tabs"
      "linter-twig"
      "linter-jade"
      "linter-php"
      "linter-phpcs"
      "linter-phpmd"
      "linter"
    ]
    themes: [
      "atom-material-ui"
      "ilm-shell-syntax"
    ]
  docblockr:
    param_description: false
    return_description: false
  editor:
    fontFamily: "Inconsolata for Powerline, Fira Code, Monaco, monospace"
    fontSize: 16
    invisibles: {}
    scrollPastEnd: true
    showIndentGuide: true
    tabLength: 4
  "exception-reporting":
    userId: "2dd7b5fe-b3c0-ee91-59c1-98900691a247"
  "file-watcher":
    promptWhenFileHasChangedOnDisk: false
  linter:
    errorPanelHeight: 316
  "linter-php":
    executablePath: "/usr/local/php5/bin/php"
  "linter-phpcs":
    ignorePatterns: [
      "*.blade.php"
      "*.twig"
      "database/"
    ]
  "linter-phpmd": {}
  "linter-twig": {}
  "relative-numbers": {}
  "terminal-plus":
    style:
      animationSpeed: 1.5
  "tree-view":
    showOnRightSide: true
    squashDirectoryNames: true
  "vim-mode": {}
  welcome:
    showOnStartup: false
".blade.html.php.text":
  editor:
    commentEnd: " --}}"
    commentStart: "{{-- "
".css.source.twig":
  editor:
    tabLength: 4
".html.text.twig":
  editor:
    tabLength: 4
".jade.source":
  editor:
    tabLength: 4
".js.source.twig":
  editor:
    tabLength: 4
".json.source.twig":
  editor:
    tabLength: 4
".pyjade.source":
  editor:
    tabLength: 4
".source.twig":
  editor:
    tabLength: 4
".source.twig.yaml":
  editor:
    tabLength: 4
".text.twig.xml":
  editor:
    tabLength: 4
